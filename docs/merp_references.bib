	file = {ScienceDirect Snapshot:C\:\\Users\\London\\Zotero\\storage\\7L68TCXH\\S000334720900236X.html:text/html},
}

@article{Dange.etal.2021,
	title = {To group or not to group: group size dynamics and intestinal parasites in {Indian} peafowl populations},
	volume = {24},
	issn = {1437-9546},
