	keywords = {Adult, Animals, Cognition, Differential Threshold, Discrimination Learning, Female, Humans, Macaca mulatta, Male, Mathematics, Neuropsychological Tests, Photic Stimulation, Species Specificity, Visual Perception, Young Adult},
	pages = {333--346},
}

@article{Hanus.Call.2007,
